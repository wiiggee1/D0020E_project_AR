extends header.pug
block linkJs
    script(src="/js/courses.js")
block content
    div#midbox
        div#infobox 
            div#selects
                select#startingpointCourses
                    option(value='0', selected=true) --Choose starting point--
                    option(value='1') Starting point 1
                    option(value='2') Starting point 2
                    option(value='3') Starting point 3
                    option(value='4') Starting point 4
                    option(value='5') Starting point 5
                div#imgContainer
                    img#image(src=courseLink)